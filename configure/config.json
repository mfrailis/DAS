[
    {
	"host"          : "localhost",
	"port"          : 3306,
	"db_type"  	: "mysql",
	"alias"         : "ciws",
	"mysql_socket"  : "/var/run/mysqld/mysqld.sock",
	"db_name"  	: "ciws_prototype",
	"ddl"	     	: "das_ddl_prototype.xml",
	"storage_engine": {
	    "name"          : "Raw",
	    "root_dir"      : "%$HOME$%/data/ciws",
	    "default_path"  : "%F/%t/%n/%v/",
	    "custom_path"   : "%s/%B/%n/%v",
	    "temp_path"      : "%$HOME$/data/temp/%$LOGNAME$",
	    "unref_data_expiration_time" : 86400
	}
    }
]
